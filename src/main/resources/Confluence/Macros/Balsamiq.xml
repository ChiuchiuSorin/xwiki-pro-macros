    <property>
      <code>{{velocity}}
  ## Macro parameters
  #set($resourceID = $xcontext.macro.params.get('initialResourceID'))
  #set($initialBranchID = $xcontext.macro.params.get('initialBranchID'))
  #set($alignment = $xcontext.macro.params.get('Alignment'))
  ## PNG attachment details
  #set($attchmentNamePrefix = 'balsamiq')
  #set($attchmentNameSuffix = 'Master')
  #if("$!initialBranchID" != '')
    #set($attchmentNameSuffix = $initialBranchID)
  #end
  #set($imageAlignment = 'center')
  #if("$!alignment" != '')
    #set($imageAlignment = $alignment.toLowerCase())
  #end
  #set($resourceAttchmentName = "${attchmentNamePrefix}_${resourceID}_${attchmentNameSuffix}.png")
  #set($resourceAttachment = $doc.getAttachment($resourceAttchmentName))
  ## Display
  #if($resourceAttachment)
    (% style="text-align: ${imageAlignment}"%)(((
      image:${resourceAttchmentName}
    )))
  #end
{{/velocity}}</code>
