    <property>
      <code>{{velocity output="false"}}
#macro(displayModal $id $diagram)

  {{html clean="false"}}
    &lt;!-- Modal --&gt;
    &lt;div class="modal fade" id="gliffy-${id}-modal" tabindex="-1" role="dialog" aria-labelledby="gliffy-${id}-title" aria-hidden="true"&gt;
      &lt;div class="modal-dialog modal-dialog-centered" role="document"&gt;
        &lt;div class="modal-content"&gt;
          &lt;div class="modal-header"&gt;
            &lt;div class="modal-title" id="gliffy-${id}-title"&gt;$services.localization.render('gliffy.diagram.modal.title')&lt;/div&gt;
            &lt;button type="button" class="close" data-dismiss="modal" aria-label="Close"&gt;
              &lt;span aria-hidden="true"&gt;&amp;times;&lt;/span&gt;
            &lt;/button&gt;
          &lt;/div&gt;
          &lt;div class="modal-body"&gt;
            $services.localization.render('gliffy.diagram.modal.text')
          &lt;/div&gt;
          &lt;div class="modal-footer"&gt;
            &lt;button type="button" class="btn btn-secondary" data-dismiss="modal"&gt;Close&lt;/button&gt;
            &lt;form action="" method="post"&gt;
              &lt;input name="action" value="createDiagram" type="hidden" /&gt;
              &lt;input name="diagram" class="delete-diagram-page" type="hidden" value="$escapetool.xml($diagram)"/&gt;
              &lt;button type="submit" class="btn btn-primary delete-diagram-btn"&gt;$services.localization.render("gliffy.diagram.modal.btn.confirm")&lt;/button&gt;
            &lt;/form&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
  {{/html}}
#end
{{/velocity}}

{{velocity}}
  #set($discard = $xwiki.jsx.use('Confluence.Macros.Gliffy'))
  #set($discard = $xwiki.ssx.use('Confluence.Macros.Gliffy'))
  ## Macro parameters
  #set($diagramName = $xcontext.macro.params.get('name'))
  ## "Constants"
  #set($previewImageName = "${diagramName}.png")
  #set($previewImage = $doc.getAttachment($previewImageName))
  ## Variables
  #set($displayDiagram = true)
  #set($diagram = $NULL)
  ##
  ## Check if the diagram page exists
  #set($docReferenceSerialized = $services.model.serialize($doc.documentReference, 'local'))
  #set($diagrams = $services.query.xwql('from doc.object(Confluence.Macros.GliffyClass) as gliffyObj where gliffyObj.page = :page and gliffyObj.diagramName = :diagramName').bindValue('page', $docReferenceSerialized).bindValue('diagramName', $diagramName).setLimit(1).execute())
  #if($diagrams.size() &gt; 0)
    #set($diagram = $diagrams.get(0))
  #end
  ##
  (% class="gliffy-diagram-contaier" %)(((
    ## The diagram exists -&gt; display it
    #if($displayDiagram &amp;&amp; $diagram)
      {{diagram reference="$diagram" cached="false" /}}

      #if($diagram &amp;&amp; $xcontext.action != 'export')
        ##
        ## If Display a button allowing the user to go back to gliffy mode
        #if ($services.security.authorization.hasAccess('delete', $diagram))
          #set($id = $mathtool.random(1, 100000))
          $services.localization.render('gliffy.diagram.delete')

          {{html clean="false"}}
            &lt;a class="btn btn-primary" data-toggle="modal" data-target="#gliffy-${id}-modal"&gt;$services.localization.render('gliffy.diagram.delete.link')&lt;/a&gt;
          {{/html}}

          #displayModal($id $diagram)
        #end
      #end
    #else
      #set($displayDiagram = false)
    #end
    ## Display the preview image if it exists
    #if($previewImage &amp;&amp; !$displayDiagram)
      (% class="thumbnail" %)(((
        image:${previewImageName}

        ##
        ## If Display a button allowing the user to create an XWiki diagram
        #if ($services.security.authorization.hasAccess('edit', $doc.documentReference) &amp;&amp; $xcontext.action != 'export')
          (% class="caption" %)(((
            $services.localization.render('gliffy.diagram.create')

            {{html clean=false}}
              &lt;form action="" method="post"&gt;
                &lt;input name="action" value="createDiagram" type="hidden" /&gt;
                &lt;input name="pageReferenceSerialized" class="create-diagram-page" value="$escapetool.xml($docReferenceSerialized)" type="hidden" /&gt;
                &lt;input name="diagramName" class="create-diagram-name" value="$escapetool.xml($diagramName)" type="hidden" /&gt;
                &lt;button type="submit" class="btn btn-primary create-diagram-btn"&gt;$services.localization.render('gliffy.diagram.create.confirm')&lt;/button&gt;
              &lt;/form&gt;
            {{/html}}
          )))
        )))
      #end
    #end
  )))
{{/velocity}}</code>
